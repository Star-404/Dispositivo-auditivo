programa
{
	funcao inicio() 
	{

	real a, b

	escreva ("insira o seu numero: ")
	leia (a)
	escreva ("insira o seu segundo numero: ")
	leia (b)

	se( a > b )

     escreva ("o numero maior é: ", a)

	senao se( a < b)

	escreva("o numero maior é: ", b)

	senao

	escreva ("os numeros são iguais")
 
	}

}
